{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "OpenTelemetry": {
    "Endpoint": "http://jaeger:4317"
  },
  "Browser": {
    "Headless": true,
    "Timeout": 30000,
    "UserAgent": "CodingAgent/2.0",
    "BlockImages": false,
    "BlockCSS": false
  }
}
